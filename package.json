{
    "name": "lazy-lang",
    "displayName": "lazy",
    "description": "Language support for the Lazy Lang",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "lazy",
            "aliases": ["lazy", "lazy-lang"],
            "extensions": [".lazy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lazy",
            "scopeName": "source.lazy",
            "path": "./syntaxes/rust.tmLanguage.json"
        }],
          "snippets": [
      {
        "language": "lazy",
        "path": "./snippets/lazy.json"
      }
    ]
    }
}
